{
  "name": "klasse",
  "version": "1.0.5",
  "description": "A minimal class/mixin utility",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "browserify": "mkdir -p build && browserify ./index.js --standalone klasse --outfile build/klasse.js",
